<%= form_with(model: @sale, local: true) do |f|%>
<%= f.label :cod %>
<%= f.text_field :cod %>

<%= f.label :detail %>
<%= f.text_area :detail %>

<%= f.label :category %>
<%= f.text_field :category %>

<%= f.label :value %>
<%= f.text_field :value %>

<%= f.label :discount %>
<%= f.text_field :discount %>

<%= f.label :'Exento de IVA' %>
<%= f.check_box :tax %>

<%= f.submit :crear %>
<% end %>